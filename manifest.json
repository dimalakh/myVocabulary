{
  "name": "myVocabulary",
  "description": "This extension helps to save unknown words and learn them.",
  "version": "1.0",
  "icons": {
    "48": "icon/48.png"
  },
  "manifest_version": 2,
  "minimum_chrome_version": "31.0",
  "permissions": [
    "storage",
    "tabs"
  ],
  "browser_action": {
    "default_title": "Open vocabulary",
    "default_icon": "icon/48.png",
    "default_popup": "popup.html"
  },
  "background": {
		"scripts": ["dist/background.js"]
  }
}
